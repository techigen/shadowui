#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/REPO/shadow-ui/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/bin/node_modules:/mnt/d/REPO/shadow-ui/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/node_modules:/mnt/d/REPO/shadow-ui/node_modules/.pnpm/typescript@5.0.4/node_modules:/mnt/d/REPO/shadow-ui/node_modules/.pnpm/node_modules:/mnt/d/REPO/shadow-ui/node_modules:/mnt/d/REPO/node_modules:/mnt/d/node_modules:/mnt/d/REPO/shadow-ui/node_modules/typescript/bin/node_modules:/mnt/d/REPO/shadow-ui/node_modules/typescript/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/REPO/shadow-ui/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/bin/node_modules:/mnt/d/REPO/shadow-ui/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/node_modules:/mnt/d/REPO/shadow-ui/node_modules/.pnpm/typescript@5.0.4/node_modules:/mnt/d/REPO/shadow-ui/node_modules/.pnpm/node_modules:/mnt/d/REPO/shadow-ui/node_modules:/mnt/d/REPO/node_modules:/mnt/d/node_modules:/mnt/d/REPO/shadow-ui/node_modules/typescript/bin/node_modules:/mnt/d/REPO/shadow-ui/node_modules/typescript/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
